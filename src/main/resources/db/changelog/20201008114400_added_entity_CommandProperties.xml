<?xml version="1.0" encoding="utf-8"?>
<databaseChangeLog xmlns="http://www.liquibase.org/xml/ns/dbchangelog"
                   xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
                   xsi:schemaLocation="http://www.liquibase.org/xml/ns/dbchangelog
                      http://www.liquibase.org/xml/ns/dbchangelog/dbchangelog-3.8.xsd">
    <changeSet id="20201008114400-1" author="stdmk">
        <createTable tableName="commandproperties" schemaName="bot">
            <column name="id" autoIncrement="true"  type="bigint">
                <constraints primaryKey="true" nullable="false"/>
            </column>
            <column name="commandname" type="varchar(255)">
                <constraints nullable="true" />
            </column>
            <column name="russifiedname" type="varchar(255)">
                <constraints nullable="true" />
            </column>
            <column name="enruname" type="varchar(255)">
                <constraints nullable="true" />
            </column>
            <column name="classname" type="varchar(255)">
                <constraints nullable="true" />
            </column>
            <column name="accesslevel" type="int">
                <constraints nullable="true" />
            </column>
            <column name="help" type="varchar(255)">
                <constraints nullable="true" />
            </column>
        </createTable>
    </changeSet>
</databaseChangeLog>
